{
    "name": "Angular (MrStickyPiston.is-a.dev)",
    "image": "mcr.microsoft.com/vscode/devcontainers/javascript-node",
    "features": {
        "node": "22"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "angular.ng-template"
            ]
        }
    },
    "postCreateCommand": "npm install -g @angular/cli && npm i",
    "forwardPorts": [
        4200
    ],
    "portsAttributes": {
        "4200": {
            "label": "Angular"
        }
    }
}